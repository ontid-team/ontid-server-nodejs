{
  "name": "skeleton-nodejs",
  "version": "0.2.1",
  "description": "Skeleton server",
  "main": "src/index.ts",
  "scripts": {
    "start:dev": "tsc-watch --onSuccess \"ts-node ./src/index.ts\" | pino-pretty",
    "start:prod": "node build/index.js | pino-pretty",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "eslint \"src/**/*.ts\" --fix",
    "test:unit": "mocha -r ts-node/register tests/unit/**/*.test.ts",
    "test:integration": "mocha -r ts-node/register tests/integration/**/*.test.ts",
    "test:e2e": "mocha -r ts-node/register tests/e2e/**/*.test.ts",
    "coverage:unit": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test:unit",
    "coverage:integration": "nyc -r lcov -e .ts -x \"*.test.ts\" test:integration",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migrate:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "migrate:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migrate:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "seed:run:s": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -s",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ontid-team/skeleton-nodejs.git"
  },
  "author": "Ontid/Neverovski",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ontid-team/skeleton-nodejs/issues"
  },
  "engines": {
    "node": "^16",
    "npm": "^8"
  },
  "homepage": "https://github.com/ontid-team/skeleton-nodejs#readme",
  "dependencies": {
    "@fullerstack/nax-ipware": "^0.10.0",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "bull": "^4.8.3",
    "class-transformer": "^0.5.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-pino-logger": "^7.0.0",
    "express-prom-bundle": "^6.4.1",
    "faker": "^5.5.3",
    "helmet": "^5.0.2",
    "joi": "^17.6.0",
    "json-schema": "^0.4.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.1.2",
    "mime": "^3.0.0",
    "ms": "^2.1.3",
    "multer": "^1.4.4",
    "nanoid": "^3.3.4",
    "nodemailer": "^6.7.5",
    "pg": "^8.7.3",
    "pino": "^7.11.0",
    "pino-pretty": "^7.6.1",
    "pino-seq": "^0.9.0",
    "prom-client": "^14.0.1",
    "qs": "^6.10.3",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.2.45",
    "typeorm-seeding": "^1.6.1",
    "ua-parser-js": "^1.0.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/bull": "^3.15.8",
    "@types/chai": "^4.3.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-pino-logger": "^4.0.3",
    "@types/faker": "^5.5.9",
    "@types/json-schema": "^7.0.11",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mime": "^2.0.3",
    "@types/mocha": "^9.1.1",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.33",
    "@types/nodemailer": "^6.4.4",
    "@types/pg": "^8.6.5",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "prettier": "^2.6.2",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.6.7",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4"
  }
}
