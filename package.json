{
  "name": "skeleton-nodejs",
  "version": "0.1.1",
  "description": "Skeleton server",
  "main": "src/index.ts",
  "scripts": {
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"ts-node -r tsconfig-paths/register ./src/index.ts\" | pino-pretty",
    "start:prod": "node build/index.js | pino-pretty",
    "build": "tsc -p tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./build",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "eslint \"src/**/*.ts\" --fix",
    "test:unit": "mocha -r ts-node/register -r tsconfig-paths/register tests/unit/**/*.test.ts",
    "test:integration": "mocha -r ts-node/register -r tsconfig-paths/register tests/integration/**/*.test.ts",
    "coverage:unit": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test:unit",
    "coverage:integration": "nyc -r lcov -e .ts -x \"*.test.ts\" test:integration",
    "migrate:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create -n",
    "migrate:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "migrate:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ontid-team/skeleton-nodejs.git"
  },
  "author": "Ontid/Neverovski",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ontid-team/skeleton-nodejs/issues"
  },
  "engines": {
    "node": "^14",
    "npm": "^6"
  },
  "homepage": "https://github.com/ontid-team/skeleton-nodejs#readme",
  "dependencies": {
    "ajv": "^8.6.2",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.0.0",
    "auto-bind": "^4.0.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "bull": "^3.29.1",
    "class-transformer": "^0.4.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dayjs": "^1.10.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "helmet": "^4.6.0",
    "joi": "^17.4.2",
    "json-schema": "^0.3.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.4",
    "mime": "^2.5.2",
    "ms": "^2.1.3",
    "multer": "^1.4.3",
    "nanoid": "^3.1.25",
    "nodemailer": "^6.6.3",
    "pg": "^8.7.1",
    "pino": "^6.13.1",
    "pino-pretty": "^6.0.0",
    "qs": "^6.10.1",
    "redis": "^3.1.2",
    "typeorm": "^0.2.37",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@types/body-parser": "^1.19.1",
    "@types/bull": "^3.15.4",
    "@types/chai": "^4.2.21",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.8",
    "@types/json-schema": "^7.0.9",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mime": "^2.0.3",
    "@types/mocha": "^9.0.0",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.7.8",
    "@types/nodemailer": "^6.4.4",
    "@types/pg": "^8.6.1",
    "@types/pino": "^6.3.11",
    "@types/qs": "^6.9.7",
    "@types/redis": "^2.8.31",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.2",
    "cz-conventional-changelog": "3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sort-class-members": "^1.11.0",
    "husky": "^7.0.2",
    "mocha": "^9.1.1",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.2.1",
    "tsc-watch": "^4.5.0",
    "tsconfig-paths": "^3.11.0",
    "tscpaths": "0.0.9",
    "typescript": "^4.4.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
